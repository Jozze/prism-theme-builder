<!-- I'm a comment -->
And i'm not

<!-- Empty tag -->
<p></p>

<!-- Tag that spans multiple lines -->
<p
>hello!
</p>

<!-- Name-attribute pair -->
<p></p>

<!-- Name-attribute pair without quotes -->
<p class=prism></p>

<!-- Attribute without value -->
<p data-foo></p>
<p data-foo ></p>

<!-- Namespaces -->
<html:p foo:bar="baz" foo:weee></html:p>

<!-- XML prolog -->
<?xml version="1.0" encoding="utf-8"?>
<svg></svg>

<!-- DOCTYPE -->
<!DOCTYPE html>
<html></html>

<!-- CDATA section -->
<ns1:description><![CDATA[
  CDATA is <not> magical.
]]></ns1:description>


<!-- Entities -->
&amp; &#x2665; &#160; &#x152;

<!-- Embedded JS and CSS -->
<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<title>I can haz embedded CSS and JS</title>
	<style>
		@media print {
			p { color: red !important; }
		}
	</style>
</head>
<body>
	<h1>I can haz embedded CSS and JS</h1>
	<script>
	if (true) {
		console.log('foo');
	}
	</script>

</body>
</html>

<!-- Invalid HTML -->
<l </ul>

<!-- Multi-line attribute values -->
<p title="foo
bar
baz">